{
  "author": "planttheidea",
  "ava": {
    "concurrency": 10,
    "babel": {
      "babelrc": true,
      "presets": [
        "react"
      ]
    },
    "failFast": true,
    "files": [
      "test/utils/*.js",
      "test/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/crio/issues"
  },
  "dependencies": {
    "core-js": "~2.4.1",
    "hash-it": "~2.0.1",
    "lodash": "~4.16.4",
    "stringifier": "~1.3.0"
  },
  "description": "Immutable objects and arrays in a natural way",
  "devDependencies": {
    "ava": "0.16.0",
    "babel": "6.5.2",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "eslint": "3.9.0",
    "eslint-friendly-formatter": "2.0.6",
    "eslint-loader": "1.6.0",
    "html-webpack-plugin": "2.24.0",
    "immutable": "3.8.1",
    "in-publish": "2.0.0",
    "mori": "0.3.2",
    "nyc": "8.3.2",
    "onchange": "3.0.2",
    "optimize-js-plugin": "0.0.4",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "rimraf": "^2.5.4",
    "seamless-immutable": "6.3.0",
    "sinon": "1.17.6",
    "webpack": "1.13.3",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "1.16.2"
  },
  "homepage": "https://github.com/planttheidea/crio#readme",
  "keywords": [
    "immutable",
    "crio"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "crio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/crio.git"
  },
  "scripts": {
    "benchmark": "npm run transpile && NODE_ENV=production node ./benchmarks/index.js",
    "benchmark:watch": "onchange -i 'src/**/*.js' -- npm run benchmark",
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack.config.minified.js",
    "clean": "rimraf lib && rimraf dist",
    "compile-for-publish": "npm run clean && npm run lint && npm test && npm run transpile && npm run dist",
    "dev": "NODE_ENV=development webpack-dev-server --profile --colors --config=webpack.config.dev.js",
    "dev:production": "NODE_ENV=production webpack-dev-server --progress --colors --config=webpack.config.dev.js",
    "dist": "npm run build && npm run build:minified",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run compile-for-publish || in-install",
    "test": "NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_ENV=test ava --watch",
    "transpile": "./node_modules/babel-cli/lib/babel/index.js -d lib/ src/"
  },
  "version": "3.4.0"
}
