{
  "author": "planttheidea",
  "ava": {
    "concurrency": 10,
    "babel": {
      "babelrc": true,
      "presets": [
        "react"
      ]
    },
    "failFast": true,
    "files": [
      "test/utils/*.js",
      "test/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/crio/issues"
  },
  "dependencies": {
    "deep-freeze-strict": "^1.1.1",
    "hash-it": "^2.1.2",
    "lodash": "^4.17.4",
    "stringifier": "~1.3.0"
  },
  "description": "Immutable objects and arrays in a natural way",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel": "6.5.2",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.14.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "html-webpack-plugin": "^2.26.0",
    "immutable": "3.8.1",
    "in-publish": "2.0.0",
    "lodash-webpack-plugin": "^0.11.0",
    "mori": "0.3.2",
    "nyc": "^10.1.2",
    "onchange": "^3.2.1",
    "optimize-js-plugin": "0.0.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "seamless-immutable": "^7.0.1",
    "sinon": "^1.17.7",
    "webpack": "^1.14.0",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "1.16.2"
  },
  "homepage": "https://github.com/planttheidea/crio#readme",
  "keywords": [
    "immutable",
    "crio"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "crio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/crio.git"
  },
  "scripts": {
    "benchmark": "npm run transpile:old && npm run transpile && NODE_ENV=production node ./benchmarks/index.js",
    "benchmark:watch": "onchange -i 'src/**/*.js' -- npm run benchmark",
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack.config.minified.js",
    "clean": "rimraf lib && rimraf dist",
    "dev": "NODE_ENV=development webpack-dashboard -p 3210 -- webpack-dev-server --profile --colors --config=webpack.config.dev.js",
    "dev:production": "NODE_ENV=production webpack-dev-server --progress --colors --config=webpack.config.dev.js",
    "dist": "npm run build && npm run build:minified",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run prepublish:compile || echo ''",
    "prepublish:compile": "npm run clean && npm run lint && npm test && npm run transpile && npm run dist",
    "test": "NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_ENV=test ava --watch",
    "transpile": "babel src --out-dir lib",
    "transpile:old": "babel src-old --out-dir lib-old"
  },
  "version": "4.0.0"
}
